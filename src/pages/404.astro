import getTags from "../scripts/getTags";

const posts = await Astro.glob("../posts/*.md");

const tags = getTags(posts);
---

<!doctype html>
<html lang="en-us">
	<BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION} />
	<body>
		<main>
			404: Page not found
			<p>
				<a href="/">Go back home</a>
			</p>
		</main>
		<Footer />
	</body>
</html>
